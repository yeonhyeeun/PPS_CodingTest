#include <stdio.h>
#include <string.h>
#include <ctype.h> 

// 1. ë¬¸ìì—´ ê¸¸ì´ê°€ 4 ë˜ëŠ” 6ì¸ì§€ í™•ì¸
// 2. ìˆ«ìë¡œë§Œ ì´ë£¨ì–´ì¡ŒëŠ”ì§€ í™•ì¸
// 3. ì¡°ê±´ì„ ëª¨ë‘ ë§Œì¡±í•˜ë©´ True ë°˜í™˜

int solution(const char* s) {
    int length = strlen(s); 

    if (length != 4 && length != 6) {
        return 0; 
    }

    for (int i = 0; i < length; i++) {
        if (!isdigit(s[i])) { // ìˆ«ìê°€ ì•„ë‹Œ ë¬¸ìê°€ ìˆìœ¼ë©´
            return 0; // False ë°˜í™˜
        }
    }

    return 1; // True ë°˜í™˜
}

int main() {
    // í…ŒìŠ¤íŠ¸
    printf("%d\n", solution("1234"));  
    printf("%d\n", solution("a234"));  
    printf("%d\n", solution("12345"));  
    printf("%d\n", solution("000000")); 
    return 0;

//ì½”ë“œë¦¬ë·° ì‘ì„± ğŸ“š
//ë‚˜ëŠ” ë¹„ìŠ·í•œ ë¬¸ì œë¥¼ pythonìœ¼ë¡œ í’€ì—ˆë˜ê²ƒ ê°™ê¸°ë„ í•œë° cë¡œ í’€ë• ì–´ë–¤ ë©”ì„œë“œê°€ ìˆëŠ”ì§€ í™•ì¸í•˜ê¸° ì¢‹ì€ ì½”ë“œì˜€ë‹¤

}
